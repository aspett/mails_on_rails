<div id="mailroute_container">
<table class='table'>
    <thead>
      <tr>
        <th>Date</th>
        <th>Event Type</th>
        <th>Details</th>
      </tr>
    </thead>
    <tbody>
    <% @business_events.each do |business_event| %>
      <tr>
        <td><%= business_event.date.to_s(:db) %></td>
        <td><%= ["Route Update", "Mail processed", "Discontinued Route", "New Route"][business_event.event_type] %></td>
        <td><%= business_event.details %></td>
      </tr>
    <% end %>
    </tbody>